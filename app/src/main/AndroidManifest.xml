<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.music">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NowPlaying"
            android:label="@string/now_playing"
            android:parentActivityName=".AllSongs" />
        <activity
            android:name=".ArtistTop10"
            android:label="@string/artisttop10"
            android:parentActivityName=".Artists" />
        <activity
            android:name=".Artists"
            android:label="@string/artists"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AllSongs"
            android:label="@string/allsongs"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>